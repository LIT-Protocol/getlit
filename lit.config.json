{
  "releaseStage":"Beta",
  "projectName": "lit_actions",
  "templatesRoot": "templates",
  "selectedTemplate": "ts",
  "buildConfig": {
    "defaultSrcFileName": "main.action",
    "inFileExt": ".ts",
    "outFileExt": ".js",
    "srcDir": "src",
    "outDir": "out",
    "testDir": "test"
  },
  "schemaPaths": {
    "litActionSchema": "$schemas/lit-action-schema.json"
  },
  "tempServerPort": 1210,
  "sizeLimitBytes": 1048576,
  "defaultSrcTemplate": "templates/template.action.ts",
  "defaultTestTemplate": "templates/template.t.action.mjs",
  "configFile": "getlit.json",
  "sources":{
    "types": "https://actions-docs.litprotocol.com/types.d.ts"
  },
  "createNextPagesTemplate": "next-pages",
  "createNextAppTemplate": "next-app"
}
